#ifndef __com_sun_star_io_XActiveDataSink_idl__ 
#define __com_sun_star_io_XActiveDataSink_idl__ 
 
#ifndef __com_sun_star_uno_XInterface_idl__ 
#include <com/sun/star/uno/XInterface.idl> 
