#define com_sun_star_chart2_AxisType_idl

module com
{
module sun
{
module star
{
module chart2
{

constants AxisType
{
    /** the axis represent real numbers
     */
    const long REALNUMBER = 0; 
    /** the axis represent real numbers in percent
     */
