    // DocMerge from xml: service com::sun::star::sdb::Connection: interface com::sun::star::sdb::XCommandPreparation
    /** interface for complex command execution.
     */
    interface XCommandPreparation; 
 
     
    // DocMerge from xml: service com::sun::star::sdb::Connection: service com::sun::star::sdbcx::DatabaseDefinition
    /** access to the database definition information of the connection.
     */
    service com::sun::star::sdbcx::DatabaseDefinition; 
 
     
    // DocMerge from xml: service com::sun::star::sdb::Connection: interface com::sun::star::sdb::XQueriesSupplier
    /** returns a container of associated queries.
     */
    interface XQueriesSupplier; 
 
     
    // DocMerge from xml: service com::sun::star::sdb::Connection: interface com::sun::star::sdb::XSQLQueryComposerFactory
    /** returns a tool for composing queries.
     */
    interface XSQLQueryComposerFactory; 
}; 
 
